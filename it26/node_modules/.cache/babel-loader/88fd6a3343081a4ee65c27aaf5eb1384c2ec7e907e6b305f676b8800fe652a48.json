   </el-table>\n\n    <div style=\"margin: 10px 0; display: flex\">\n      <!--      Dialog   -->\n      <el-dialog\n          v-model=\"Dialogbook\"\n          title=\"Tips\"\n          width=\"30%\"\n          align-center\n      >\n        <span>Do you confirm the booking?</span>\n        <template #footer>\n        <span class=\"dialog-footer\">\n          <el-button type=\"primary\" @click=\"confirm\">\n            Confirm\n          </el-button>\n          <el-button @click=\"Dialogbook = false\">Cancel</el-button>\n        </span>\n        </template>\n      </el-dialog>\n    </div>\n\n  </div>\n\n</template>\n\n<script>\nimport request from \"@/utils/request\";\n\nexport default {\n  name: \"Booking\",\n  components: {\n\n  },\n  data() {\n    return {\n      search: '',\n      timing: '',\n      Dialogbook: false,\n      tableData:  [],\n      form: {}\n    }\n\n  },\n  created() {\n    this.load()\n  },\n\n  methods: {\n    load() {\n      request.get(\"/booking/\",{\n        params: {\n          search: this.search,\n          timing: this.timing\n        }\n      }).then(res => {\n        console.log(res)\n        // const tableJson = res.data\n        // let table = JSON.parse(JSON.stringify(tableJson))[0][\"fields\"]\n        // console.log(table)\n        // table[\"name\"] = JSON.parse(JSON.stringify(tableJson))[0][\"pk\"]\n        // this.tableData = JSON.parse(table)\n        this.tableData = res.data\n      })\n    },\n    book(id)\n    {\n      this.Dialogbook = true\n      const userJson = sessionStorage.getItem(\"user\") || \"{}\";\n      let user=JSON.parse(JSON.parse(userJson))[0][\"fields\"]\n      user[\"id\"]=JSON.parse(JSON.parse(userJson))[0][\"pk\"]\n      // user[\"id\"]=userJson[0][\"pk\"]\n      this.form.userid = JSON.parse(JSON.stringify(user.id ));\n      this.form.movieid = id\n      console.log(this.form);\n    },\n    confirm()\n    {\n      console.log(this.form)\n      request.post(\"/booking/\", this.form).then(res => {\n   